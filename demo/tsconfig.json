{
  "compilerOptions": {
    "module": "ES2020",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "target": "ES2020",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": false,
    "lib": ["DOM", "ES2020"],
    "baseUrl": ".",
    "outDir": ".",
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    }
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": []
}
